  <!-- =========================================================================
       Interface definitions
  ========================================================================== -->
  <xsd:complexType name="IntefaceDefType">
    <xsd:annotation>
      <xsd:documentation>
        The 'InterfaceDefType' type serves as the implementation of the
        specified interface. It refers to the interface Id and it defines
        (implements) all its interface attributes.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:IntefaceRefIdType">
        <xsd:sequence>
          <xsd:element ref="ber:description" minOccurs="0"/>
          <xsd:element name="interfaceAttributeDef" maxOccurs="unbounded">
            <xsd:annotation>
              <xsd:documentation>
                The 'interfaceAttributeDef' element serves as the implementation
                of the specified interface attribute. It refers to interface
                attribute Id and it defines either default value or a reference
                to feature attribute. The data type of feature attributte has to
                correspond with data type given in the specified interface
                attribute.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:InterfaceAttributeRefIdType">
                  <xsd:choice>
                    <xsd:element name="defaultValue" type="xsd:string">
                      <xsd:annotation>
                        <xsd:documentation>
                          The 'defaultValue' element defines a default value for
                          an interface attribute.
                          NOTE: The data type of default value has to correspond
                          with data type given in interface attribute.
                        </xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="attribute"
                                 type="ber:FeatureAttributeRefIdType">
                      <xsd:annotation>
                        <xsd:documentation>
                          The 'attribute' element refers to feature attribute Id.
                          NOTE: The data type of feature attribute has to
                          correspond with data type given in interface attribute.
                        </xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:choice>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       BackdropType definitions
  ========================================================================== -->
  <xsd:element name="_backdropType" abstract="true"
               type="ber:AbstractBackdropTypeType">
    <xsd:annotation>
      <xsd:documentation>
        The '_backdropType' element defines the base abstract substituable
        element for backdrop types.
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <xsd:complexType name="AbstractBackdropTypeType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>
        The 'AbstractBackdropTypeType' is the base abstract type for all
        backdrop types.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:sequence>
          <xsd:element name="featureType" minOccurs="0" type="ber:FeatureTypeRefIdType">
            <xsd:annotation>
              <xsd:documentation>Optional reference to related feature type.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="versioned" type="xsd:boolean">
          <xsd:annotation>
            <xsd:documentation>Identifies whether or not this backdrop type is versioned.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AbstractRasterBackdropTypeType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>
        The abstract complex type 'AbstractRasterBackdropTypeType' extends
        'AbstractBackdropTypeType' and represents a raster backdrop type with
        one or more layers (tiled, hierarchical or plain) defining one of the
        available raster format.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractBackdropTypeType">
        <xsd:sequence>
          <xsd:element name="layerArray">
            <xsd:annotation>
              <xsd:documentation>
                This element contains one or more raster layers. It can contain
                either one or more tiled layers with main tiled layer or one or
                more plain layers.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:choice>
                <xsd:sequence>
                  <xsd:element name="mainTiledLayer">
                    <xsd:annotation>
                      <xsd:documentation>
                        This element represents main tiled layer for raster
                        backdrop type and contains id, name and raster format.
                        It can also contain hierarchical layer with hierarchy
                        parameters
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:RasterLayerType">
                          <xsd:sequence>
                            <xsd:element name="hierarchicalLayer" minOccurs="0" maxOccurs="unbounded">
                              <xsd:annotation>
                                <xsd:documentation>
                                  This element represents hierarchical layer for
                                  raster backdrop type and contains id, name,
                                  raster format and hierarchical parameters.
                                </xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                <xsd:complexContent>
                                  <xsd:extension base="ber:RasterLayerType">
                                    <xsd:sequence>
                                      <xsd:element name="hierarchy">
                                        <xsd:annotation>
                                          <xsd:documentation>
                                            Defines parameters for hierarchy
                                            layer.
                                          </xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:complexType>
                                          <xsd:attribute name="decreaseRatio"
                                                         use="required">
                                            <xsd:annotation>
                                              <xsd:documentation>
                                                Specifies the decrease ratio.
                                                It's positive number greater
                                                than 2.
                                              </xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:simpleType>
                                              <xsd:restriction base="xsd:unsignedByte">
                                                <xsd:minInclusive value="2"/>
                                              </xsd:restriction>
                                            </xsd:simpleType>
                                          </xsd:attribute>
                                          <xsd:attribute name="tileWidth"
                                                         use="required"
                                                         type="xsd:positiveInteger">
                                            <xsd:annotation>
                                              <xsd:documentation>
                                                Specifies the width of tile in
                                                pixels.
                                              </xsd:documentation>
                                            </xsd:annotation>
                                          </xsd:attribute>
                                          <xsd:attribute name="tileHeight"
                                                         use="required"
                                                         type="xsd:positiveInteger">
                                            <xsd:annotation>
                                              <xsd:documentation>
                                                Specifies the height of tile in
                                                pixels.
                                              </xsd:documentation>
                                            </xsd:annotation>
                                          </xsd:attribute>
                                        </xsd:complexType>
                                      </xsd:element>
                                    </xsd:sequence>
                                    <xsd:attribute name="date" type="ber:DateTimeValueType"/>
                                  </xsd:extension>
                                </xsd:complexContent>
                              </xsd:complexType>
                            </xsd:element>
                          </xsd:sequence>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="tiledLayer" type="ber:RasterLayerType"
                               minOccurs="0" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>
                        This element represents tiled layer for raster backdrop
                        type and contains id, name and raster format.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
                <xsd:element name="plainLayer"
                             maxOccurs="unbounded">
                  <xsd:annotation>
                    <xsd:documentation>
                      This element represents plain layer for raster backdrop
                      type and contains id, name and raster format.
                    </xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:complexContent>
                      <xsd:extension base="ber:RasterLayerType">
                        <xsd:sequence>
                          <xsd:element name="maxSize" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Optional maximum pixel size (width, height) of returned image. Should be set especially in case when similar parameters (wms:MaxWidth, wms:MaxHeight) are set for underlying WMS.</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:attribute name="width" use="required" type="xsd:integer"/>
                              <xsd:attribute name="height" use="required" type="xsd:integer"/>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:element name="maxParallelRequests" minOccurs="0" type="xsd:positiveInteger">
                            <xsd:annotation>
                              <xsd:documentation>Maximum number of requests a single client can execute in parallel.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:extension>
                    </xsd:complexContent>
                  </xsd:complexType>
                </xsd:element>
              </xsd:choice>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="vectorBackdropType" substitutionGroup="ber:_backdropType">
    <xsd:annotation>
      <xsd:documentation>
        The 'vectorBackdropType' element extends 'AbstractBackdropTypeType' and
        represents vector backdrop type with one or more layers defining the one
        of the available vector format. It can also define a symbology and
        dynamic symbology.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ber:AbstractBackdropTypeType">
          <xsd:sequence>
            <xsd:element name="layerArray">
              <xsd:annotation>
                <xsd:documentation>
                  This element contains either one or more tiled layers or one
                  or more plain layers.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:choice>
                  <xsd:element name="tiledLayer" maxOccurs="unbounded"
                               type="ber:VectorLayerType">
                    <xsd:annotation>
                      <xsd:documentation>
                        This element represents tiled layer for vector backdrop
                        type and contains id, name and vector format.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="plainLayer" maxOccurs="unbounded"
                               type="ber:VectorLayerType">
                    <xsd:annotation>
                      <xsd:documentation>
                        This element represents plain layer for vector backdrop
                        type and contains id, name and vector format.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:choice>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="symbology" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
                  The 'symbology' element contains a simple vector symbolizer.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element ref="ber:simpleVectorSymbolizer"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element ref="ber:dynamicSymbology" minOccurs="0"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="rasterBackdropType" substitutionGroup="ber:_backdropType">
    <xsd:annotation>
      <xsd:documentation>
        The 'rasterBackdropType' element represents raster backdrop type with
        one or more layers (tiled, hierarchical or plain) defining one of the
        available raster format. It can also define a symbology and dynamic
        symbology.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ber:AbstractRasterBackdropTypeType">
          <xsd:sequence>
            <xsd:element name="symbology" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
                  The 'symbology' element contains a raster symbolizer.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element ref="ber:rasterSymbolizer"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element ref="ber:dynamicSymbology" minOccurs="0"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="binaryRasterBackdropType"
               substitutionGroup="ber:_backdropType">
    <xsd:annotation>
      <xsd:documentation>
        The 'binaryRasterBackdropType' element represents binary raster backdrop
        type with one or more layers (tiled, hierarchical or plain) defining one
        of the available raster format. It can also define a symbology and
        dynamic symbology.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ber:AbstractRasterBackdropTypeType">
          <xsd:sequence>
            <xsd:element name="symbology" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
                  The 'symbology' element contains a binary raster symbolizer.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element ref="ber:binaryRasterSymbolizer"/>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element ref="ber:dynamicSymbology" minOccurs="0"/>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="RasterLayerType">
    <xsd:annotation>
      <xsd:documentation>
        This type represents the complex type for raster layers.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:attribute name="format" use="required"
                       type="ber:RasterFormatType">
          <xsd:annotation>
            <xsd:documentation>
              This attribute specifies one of the available raster formats.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="colorModelType" type="ber:ColorModelType">
          <xsd:annotation>
            <xsd:documentation>
              If specified, only images having the specified color model can be inserted into the layer.
              If not specified, images with all color models can be inserted.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="VectorLayerType">
    <xsd:annotation>
      <xsd:documentation>
        This type represents the complex type for vector layers.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:attribute name="format" use="required" type="ber:VectorFormatType">
          <xsd:annotation>
            <xsd:documentation>
              This attribute specifies one of the available raster formats.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       VicinityMap definitions
  ========================================================================== -->
  <xsd:complexType name="VicinityMapType">
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:sequence>
          <xsd:element name="range" type="ber:RangeType" minOccurs="0"/>
          <xsd:element name="backdropTypeRefs">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="backdropType" type="ber:BackdropTypeRefIdType" maxOccurs="unbounded"/>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="default" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       Category definitions
  ========================================================================== -->
  <xsd:complexType name="CategoryType">
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:sequence>
          <xsd:element name="featureIdArray" minOccurs="0">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="item" type="ber:FeatureTypeRefIdType"
                             maxOccurs="unbounded"/>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parentId" type="xsd:IDREF"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       Backdrop category definitions
  ========================================================================== -->
  <xsd:complexType name="BackdropCategoryType">
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:sequence>
          <xsd:element name="backdropTypeIdArray" minOccurs="0">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="item" type="ber:BackdropTypeRefIdType"
                             maxOccurs="unbounded"/>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="parentId" type="xsd:IDREF"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       Role definitions
  ========================================================================== -->
  <xsd:attribute name="minFeaturesInRole" type="ber:CardinalityType" default="1"/>
  <xsd:attribute name="maxFeaturesInRole" type="ber:CardinalityType" default="1"/>

  <xsd:element name="_roleType" type="ber:AbstractRoleType" abstract="true"/>
  <xsd:element name="masterRole" type="ber:One2OneRoleType"
               substitutionGroup="ber:_roleType"/>
  <xsd:element name="detailRole" type="ber:One2AnyRoleType"
               substitutionGroup="ber:_roleType"/>
  <xsd:element name="relationRole" type="ber:RelationRoleType"
               substitutionGroup="ber:_roleType"/>
  <xsd:element name="interfaceRole" type="ber:InterfaceRoleType"
               substitutionGroup="ber:_roleType"/>

  <xsd:complexType name="AbstractRoleType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>
        The 'AbstractRoleType' type is common ancestor for deriving concrete
        role types. It can contain references to feature types belong to this
        role.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:MetaItemType">
        <xsd:sequence>
          <xsd:element name="ftItem" type="ber:FeatureTypeRefIdType" maxOccurs="unbounded"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="One2OneRoleType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractRoleType">
        <xsd:attribute ref="ber:minFeaturesInRole" fixed="1"/>
        <xsd:attribute ref="ber:maxFeaturesInRole" fixed="1"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="One2AnyRoleType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractRoleType">
        <xsd:attribute ref="ber:minFeaturesInRole" fixed="1"/>
        <xsd:attribute ref="ber:maxFeaturesInRole" fixed="unbounded"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="RelationRoleType">
    <xsd:complexContent>
      <xsd:extension base="ber:One2OneRoleType">
        <xsd:attribute name="dbName" type="ber:DBNameType" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="NoFeatureTypeListRoleType" abstract="true">
    <xsd:complexContent>
      <xsd:restriction base="ber:AbstractRoleType">
        <xsd:sequence>
          <xsd:element name="description" type="xsd:string" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
                The element 'description' can be used for additional information of a meta item.
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="InterfaceRoleType">
    <xsd:complexContent>
      <xsd:extension base="ber:NoFeatureTypeListRoleType">
        <xsd:attribute ref="ber:minFeaturesInRole" fixed="1"/>
        <xsd:attribute ref="ber:maxFeaturesInRole" fixed="1"/>
        <xsd:attribute name="dbName" type="ber:DBNameType" use="required"/>
        <xsd:attribute name="interface" type="xsd:IDREF" use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       Association definitions
  ========================================================================== -->
  <xsd:complexType name="AttributesOrderByType">
    <xsd:sequence>
      <xsd:element name="attribute" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Specifies the feature attribute, direction and position for the sorting.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="ber:OrderByType">
              <xsd:attribute name="refId" type="xsd:IDREF" use="required"/>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:element name="_assocType" type="ber:AbstractAssocType" abstract="true"/>
  <xsd:element name="detailAssoc" type="ber:DetailAssocType"
               substitutionGroup="ber:_assocType"/>
  <xsd:element name="relationAssoc" type="ber:RelationAssocType"
               substitutionGroup="ber:_assocType"/>
  <xsd:element name="featureRefRelationAssoc" type="ber:FeatureRefRelationAssocType"
               substitutionGroup="ber:_assocType"/>
  <xsd:element name="spatialRelationAssoc" type="ber:SpatialRelationAssocType"
               substitutionGroup="ber:_assocType"/>
  <xsd:element name="linearTopologyAssoc" type="ber:LinearTopologyAssocType"
               substitutionGroup="ber:_assocType">
    <xsd:annotation>
      <xsd:documentation>
        The type for linear topology association.
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:element name="arealTopologyAssoc" type="ber:ArealTopologyAssocType"
               substitutionGroup="ber:_assocType"/>
  <xsd:element name="linearTopologyConnection" type="ber:LinearTopologyConnectionAssocType"
               substitutionGroup="ber:_assocType"/>

  <xsd:element name="relationRuleArray">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="rule" maxOccurs="unbounded" type="ber:RelationRuleType">
          <xsd:annotation>
            <xsd:documentation>
              The 'rule' element represents a relation rule.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- =========================================================================
       Relation rule definitions
  ========================================================================== -->
    <xsd:complexType name="RelationRuleType">
        <xsd:complexContent>
            <xsd:extension base="ber:MetaItemType">
                <xsd:sequence>
                    <xsd:element name="condition" type="ber:RelationRuleExpressionType" minOccurs="0" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>
                                The 'condition' element represents a optional condition for constraint expression evaluation.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="constraint" type="ber:RelationRuleExpressionType" maxOccurs="unbounded">
                        <xsd:annotation>
                            <xsd:documentation>
                                The 'constraint' element represents a at least one constraint expression for evaluation.
                            </xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="cascadeRollback" type="xsd:string">
                    <xsd:annotation>
                        <xsd:documentation>This attribute specifies if feature won't be created with any corrupted relation
                            rule.
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
  <!-- =========================================================================
       Relation rule expression definitions
  ========================================================================== -->
  <xsd:complexType name="RelationRuleExpressionType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="r1.roleId" type="xsd:IDREF" use="required">
          <xsd:annotation>
            <xsd:documentation>
              This attribute specifies the role type identifier for the 1-st role.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="r1.ftids">
          <xsd:annotation>
            <xsd:documentation>This attribute specifies a feature type filter for the 1-st role.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:list itemType="xsd:IDREF"/>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="r2.roleId" type="xsd:IDREF">
          <xsd:annotation>
            <xsd:documentation>
              This attribute specifies the role type identifier for the 2-nd role.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="r2.ftids">
          <xsd:annotation>
            <xsd:documentation>This attribute specifies a feature type filter for the 2-nd role.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:list itemType="xsd:IDREF"/>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>

  <xsd:complexType name="AbstractAssocType" abstract="true">
    <xsd:complexContent>
      <xsd:extension base="ber:LongNameMetaItemType">
        <xsd:sequence>
          <xsd:element ref="ber:groupsArray" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="RoleTypeRefType">
    <xsd:annotation>
      <xsd:documentation>
        Refers to roleType.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:RoleTypeRefIdType">
        <xsd:attribute name="cardinality" type="ber:CardinalityType" default="unbounded"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="DetailAssocType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractAssocType">
        <xsd:sequence>
          <xsd:element name="masterRole" type="ber:RoleTypeRefType"/>
          <xsd:element name="detailRole">
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:RoleTypeRefType">
                  <xsd:attribute name="deleteWithDetail" type="xsd:boolean" default="false"/>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="RelationAssocType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractAssocType">
        <xsd:sequence>
          <xsd:element name="srcRole">
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:RoleTypeRefType">
                  <xsd:sequence>
                    <xsd:element name="orderBy" type="ber:AttributesOrderByType" minOccurs="0"/>
                  </xsd:sequence>
                  <xsd:attribute name="deleteWithDestFeature"
                                 type="xsd:boolean" default="false">
                    <xsd:annotation>
                      <xsd:documentation>
                        This attribute identifies whether or not the related
                        feature(s) in this source role will be deleted during
                        destination feature deletion.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="destRole">
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:RoleTypeRefType">
                  <xsd:sequence>
                    <xsd:element name="orderBy" type="ber:AttributesOrderByType" minOccurs="0"/>
                  </xsd:sequence>
                  <xsd:attribute name="deleteWithSrcFeature"
                                 type="xsd:boolean" default="false">
                    <xsd:annotation>
                      <xsd:documentation>
                        This attribute identifies whether or not the related
                        feature(s) in this destination role will be deleted
                        during source feature deletion.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element ref="ber:relationRuleArray" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="dbName" type="ber:DBNameType" use="required"/>
        <xsd:attribute name="versioned" type="ber:VersionedType">
          <xsd:annotation>
            <xsd:documentation>Identifies the level of this relation type versioning.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="FeatureRefRelationAssocType">
    <xsd:annotation>
      <xsd:documentation>
        The 'FeatureRefAssocType' represents non database association type with
        2 roles in hierarchical relationship.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractAssocType">
        <xsd:sequence>
          <xsd:element name="masterRole">
            <xsd:annotation>
              <xsd:documentation>
                The masterRole defines a super role.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:RoleTypeRefType">
                  <xsd:sequence>
                    <xsd:element name="orderBy" type="ber:AttributesOrderByType" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="childRole">
            <xsd:annotation>
              <xsd:documentation>
                The childRole defines a subrole.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:RoleTypeRefType">
                  <xsd:sequence>
                    <xsd:element name="orderBy" type="ber:AttributesOrderByType" minOccurs="0"/>
                  </xsd:sequence>
                  <xsd:attribute name="deleteWithMasterFeature" type="xsd:boolean"
                                 default="false">
                    <xsd:annotation>
                      <xsd:documentation>
                        This attribute identifies whether or not features in
                        this child role should be deleted after master feature
                        deletion.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:attribute>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element ref="ber:relationRuleArray" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="dbName" type="ber:DBNameType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SpatialRelationAssocType">
    <xsd:complexContent>
      <xsd:extension base="ber:RelationAssocType">
        <xsd:sequence>
          <xsd:element name="interactionType">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:choice>
                  <xsd:element name="pointToPoint">
                    <xsd:annotation>
                      <xsd:documentation>
                        The 'pointToPoint' interaction type represents any point
                        to point spatial interaction fulfiling the given snap
                        radius.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:AbstractInteractionType">
                          <xsd:attribute name="snapRadius" type="xsd:decimal"
                                         use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies the snap radius for this interaction
                                type.
                              </xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="lineToPoint">
                    <xsd:annotation>
                      <xsd:documentation>
                        The 'lineToPoint' interaction type represents a line to
                        point spatial interaction with the given spatial
                        condition on line vertices.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:AbstractInteractionType">
                          <xsd:attribute name="snapRadius" type="xsd:decimal"
                                         use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies the snap radius for this interaction
                                type.
                              </xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                          <xsd:attribute name="spatialCondition" use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies spatial condition for line vertices
                                (allVertices, endVertices, innerVertices,
                                firstVertex, or lastVertex).
                              </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="allVertices"/>
                                <xsd:enumeration value="endVertices"/>
                                <xsd:enumeration value="innerVertices"/>
                                <xsd:enumeration value="firstVertex"/>
                                <xsd:enumeration value="lastVertex"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="lineToLine">
                    <xsd:annotation>
                      <xsd:documentation>
                        The 'lineToLine' interaction type represents a line to
                        line spatial interaction with the given spatial
                        condition on parallel lines. This interaction consider
                        master-slave relationship and resolves if the slave line
                        meets spatial condition to the master line.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:AbstractInteractionType">
                          <xsd:attribute name="spatialCondition" use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies spatial condition for two lines
                                (parallel, fullParallel, partialParallel,
                                intersects, commonVertex or lastToFirstVertex).
                              </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="parallel"/>
                                <xsd:enumeration value="fullParallel"/>
                                <xsd:enumeration value="partialParallel"/>
                                <xsd:enumeration value="intersects"/>
                                <xsd:enumeration value="commonVertex"/>
                                <xsd:enumeration value="lastToFirstVertex"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="faceToPoint">
                    <xsd:annotation>
                      <xsd:documentation>
                        The 'faceToPoint' interaction type represents a point to
                        face spatial interaction with the given spacial
                        condition.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:AbstractInteractionType">
                          <xsd:attribute name="spatialCondition" use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies all possible spatial conditions for
                                this interaction type (intersects, touches or
                                contains).
                              </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="intersects"/>
                                <xsd:enumeration value="touches"/>
                                <xsd:enumeration value="contains"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="faceToLine">
                    <xsd:annotation>
                      <xsd:documentation>
                        The 'faceToLine' interaction type represents a line to
                        face spatial interaction with the given spacial
                        condition.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:AbstractInteractionType">
                          <xsd:attribute name="spatialCondition" use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies all possible spatial conditions for
                                this interaction type (intersects, touches,
                                contains or boundaryContains).
                              </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="intersects"/>
                                <xsd:enumeration value="touches"/>
                                <xsd:enumeration value="contains"/>
                                <xsd:enumeration value="boundaryContains"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="faceToFace">
                    <xsd:annotation>
                      <xsd:documentation>
                        The 'faceToFace' interaction type represents a face to
                        face spatial interaction with the given spacial
                        condition.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:complexContent>
                        <xsd:extension base="ber:AbstractInteractionType">
                          <xsd:attribute name="spatialCondition" use="required">
                            <xsd:annotation>
                              <xsd:documentation>
                                Specifies all possible spatial conditions for this interaction type (intersects, touches, contains or overlaps).
                              </xsd:documentation>
                            </xsd:annotation>
                            <xsd:simpleType>
                              <xsd:restriction base="xsd:string">
                                <xsd:enumeration value="intersects"/>
                                <xsd:enumeration value="touches"/>
                                <xsd:enumeration value="contains"/>
                                <xsd:enumeration value="overlaps"/>
                              </xsd:restriction>
                            </xsd:simpleType>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:complexContent>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:choice>
                <xsd:element name="roleFilter" maxOccurs="2" minOccurs="0">
                  <xsd:complexType>
                    <xsd:complexContent>
                      <xsd:extension base="ber:RoleTypeRefIdType">
                        <xsd:sequence>
                          <xsd:element name="featureType"
                                       type="ber:FeatureTypeRefIdType"
                                       maxOccurs="unbounded"/>
                        </xsd:sequence>
                      </xsd:extension>
                    </xsd:complexContent>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AbstractInteractionType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>
        The compex type 'AbstractInteractionType' represents the abstract base
        type for all interaction types. It contains the snap radius.
      </xsd:documentation>
    </xsd:annotation>
  </xsd:complexType>

  <!-- =========================================================================
      Topology Associations
  ========================================================================== -->
   <xsd:complexType name="AbstractTopologyAssocType" abstract="true">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractAssocType">
        <xsd:sequence>
          <xsd:element ref="ber:spatialInfo" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="dbName" type="ber:DBNameType" use="required"/>
        <xsd:attribute name="snapRadius" type="xsd:decimal" use="required">
          <xsd:annotation>
            <xsd:documentation>
             The double tolerance value. It is the maximal value for considering
             two nodes to be identical.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
      Linear Topology Association
  ========================================================================== -->
  <xsd:complexType name="AbstractLinearTopologyAssocType" abstract="true">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractTopologyAssocType">
        <xsd:attribute name="update" type="ber:TopologyUpdateType"
                       use="required">
          <xsd:annotation>
            <xsd:documentation>
              Update flag. One of online, batch. The online
              graph are updated online, when master features are updated
              (inserted, updated, deleted). The batch flag defines the linear
              graph to be updated only in a batch mode. The online flag allows
              batch updating also.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>

      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="LinearTopologyAssocType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractLinearTopologyAssocType">
        <xsd:sequence>
          <xsd:element name="nodeRole">
            <xsd:annotation>
              <xsd:documentation>
                The node role contains one or more node types defined by a nodeType
                XML element. The nodeRole is defined inline  it means that the actual
                definition of nodeRole is situated inside the topologyAssoc XML element.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:MetaItemType">
                  <xsd:sequence>
                    <xsd:element name="nodeType" maxOccurs="unbounded">
                      <xsd:annotation>
                        <xsd:documentation>
                      The nodeType XML element aggregates feature types that are
                      considered as nodes in the graph. There can be one or more feature
                      types included in the node type definition.
                        </xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:complexContent>
                      <xsd:extension base="ber:DirectedNodeType">
                        <xsd:sequence>
                          <xsd:element name="constraint" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>
                                The undirected constraint for node type defines edges, that
                                can be connected to this node type. When the constraint element is not
                                present, node can be connected to any edge.
                              </xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element ref="ber:description" minOccurs="0"/>
                                <xsd:element name="featureType"
                                             type="ber:FeatureTypeRefIdType"
                                             maxOccurs="unbounded">
                                  <xsd:annotation>
                                    <xsd:documentation>
                                      Sublist of feature types that are included in the edge role and
                                      that can be connected to this node. If it is empty, this node
                                      cannot be connected to any feature edge.
                                    </xsd:documentation>
                                  </xsd:annotation>
                                </xsd:element>
                              </xsd:sequence>
                              <xsd:attribute name="name" type="xsd:string"/>
                            </xsd:complexType>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:extension>
                      </xsd:complexContent>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element ref="ber:pointSymbolizer" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="edgeRole">
            <xsd:annotation>
              <xsd:documentation>
                The edge role contains one or more edge types defined by a edgeType
                XML element. The edgeRole is defined inline  it means that the actual
                definition of edgeRole is situated inside the topologyAssoc XML element.
              </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:MetaItemType">
                  <xsd:sequence>
                    <xsd:element name="edgeType" maxOccurs="unbounded">
                      <xsd:annotation>
                        <xsd:documentation>
                          The edgeType  XML element aggregates feature types that are
                          considered as edges in the linear graph. There can be one or more feature
                          types included in the edge type. The edge type contains
                          two attributes systemNode and processVertices.
                        </xsd:documentation>
                      </xsd:annotation>
                      <xsd:complexType>
                        <xsd:complexContent>
                          <xsd:extension base="ber:DirectedEdgeType">
                            <xsd:sequence>
                              <xsd:element name="constraint" minOccurs="0">
                                <xsd:annotation>
                                  <xsd:documentation>
                                    The undirected constraint for edge type defines nodes, that
                                    can be connected to this edge type. When the constraint element
                                    is not present, edge can be connected to any node including
                                    system node.
                                  </xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element ref="ber:description" minOccurs="0"/>
                                    <xsd:element name="featureType"
                                                 type="ber:FeatureTypeRefIdType"
                                                 maxOccurs="unbounded"
                                                 minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>
                                          Sublist of feature types that are included in the node role
                                          and that can be connected to this edge. If it is empty, this
                                          edge cannot be connected to any feature node.
                                        </xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                  </xsd:sequence>
                                  <xsd:attribute name="allowsSystemNode" default="false"
                                    type="xsd:boolean">
                                    <xsd:annotation>
                                      <xsd:documentation>
                                        (true, false) This flag defines whether the edge can be
                                        connected to a system node  a generated node, that does not
                                        refer to any existing feature.
                                      </xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                  <xsd:attribute name="name" type="xsd:string"/>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:extension>
                        </xsd:complexContent>
                      </xsd:complexType>
                    </xsd:element>
                    <xsd:element ref="ber:lineSymbolizer" minOccurs="0"/>
                  </xsd:sequence>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="DirectedNodeType">
    <xsd:sequence>
      <xsd:element ref="ber:description" minOccurs="0"/>
      <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                   maxOccurs="unbounded"/>
      <xsd:element name="directedConstraintArray" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="directedConstraint" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>
                  The directed constraint for node type defines incoming and outgoing
                  edges, that can be connected to this node type. When the inEdge or outEdge
                  element is missing it means that all incomming edges or all outgoing
                  edges are allowed.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element ref="ber:description" minOccurs="0"/>
                  <xsd:element name="inEdge" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>
                        Contains list of feature types in the edge role that can
                        come in to the node. If the list is empty it means that no
                        feature edges can be incomming to the node.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                          maxOccurs="unbounded" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>
                              If the list is empty it means that no feature edges can
                              be incomming to the node.
                            </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="outEdge" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>
                        Contains list of feature types in the edge role that can go out
                        from the node. If the list is empty it means that no feature edges
                        can be outgoing from the node.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                          maxOccurs="unbounded" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>
                              If the list is empty it means that no feature edges can
                              be outgoing from the node.
                            </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="name" type="xsd:string"/>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="systemEdge" type="ber:SystemEdgeType" use="required">
      <xsd:annotation>
        <xsd:documentation>
        (none, all) When set to all, features with line-string geometry can
        be used as topology nodes. For each vertex of the feature a node is
        created. Nodes are connected by a system edges. When none is defined,
        features with line-string geometries cannot be used in the topology
        as nodes.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="name" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="DirectedEdgeType">
    <xsd:sequence>
      <xsd:element ref="ber:description" minOccurs="0"/>
      <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                   maxOccurs="unbounded"/>
      <xsd:element name="directedConstraintArray" minOccurs="0">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="directedConstraint" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>
                  The directed constraint for edge type defines starting and ending
                  nodes, that can be connected to this edge type. When the startNode
                  element is missing, it means that the edge can start in any node
                  including system node. When the endNode element is missing, it
                  means that the edge can end in any node including system node.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element ref="ber:description" minOccurs="0"/>
                  <xsd:element name="startNode" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>
                        Defines a list of start feature types in the node role. If
                        no feature type is defined it means that the edge cannot start
                        in any node feature. The startNode element contains one attribute
                        allowsSystemNode (true, false). This flag defines whether the
                        edge can start in a system node  a generated node, that does not
                        refer to any existing feature.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                          maxOccurs="unbounded" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>
                              If no feature type is defined it means that the edge cannot
                              start in any node feature.
                            </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute name="allowsSystemNode" default="false" type="xsd:boolean">
                        <xsd:annotation>
                          <xsd:documentation>
                            (true, false) This flag defines whether the edge can start
                            in a system node  a generated node, that does not refer to
                            any existing feature.
                          </xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="endNode" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>
                        Defines a list of end feature types in the node role for
                        the edge. If no feature type is defined it means that the
                        edge cannot end in any node feature. The endNode element contains
                        one attribute allowsSystemNode (true, false). This flag defines
                        whether the edge can end in a system node  a generated node,
                        that does not refer to any existing feature.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                          maxOccurs="unbounded" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>
                              If no feature type is defined it means that the edge cannot end
                              in any node feature.
                            </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute name="allowsSystemNode" default="false" type="xsd:boolean">
                        <xsd:annotation>
                          <xsd:documentation>
                            (true, false) This flag defines whether the edge can end
                            in a system node  a generated node, that does not refer to
                            any existing feature.
                          </xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="name" type="xsd:string"/>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="systemNode" type="ber:SystemNodeType" use="required">
      <xsd:annotation>
        <xsd:documentation>
        (all, inner, terminal) It defines the possibility
        to create system nodes after edge feature capturing.
        When set to terminal, system nodes can be created only on
        terminal vertices of a line-string feature. It means feature nodes
        are required on inner vertices. When set to inner, system nodes can
        be created only on inner vertices of a line-string feature. It means
        feature nodes are required on terminal vertices. When set to all,
        there are no restrictions where to create system nodes.
        Must be set in accordance with edge type processVertices parameter
        and constraints allowsSystemNode parameters.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="processVertices" type="ber:ProcessVerticesType"
                   use="required">
      <xsd:annotation>
        <xsd:documentation>
        (all, terminal) It defines algorithm of creating edges from feature
        geometries. When process vertices is set to all it means that
        a line-string feature is divided into several lines. There is
        an edge created from each line. When set to terminal only one
        edge is created from one line-string feature.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="name" type="xsd:string"/>
  </xsd:complexType>

  <xsd:complexType name="LinearTopologyConnectionAssocType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractAssocType">
        <xsd:sequence>
          <xsd:element name="startRole">
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:AbstractAssocType">
                  <xsd:sequence>
                    <xsd:element name="ftItem"
                                 type="ber:FeatureTypeRefIdType"/>
                  </xsd:sequence>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element name="endRole">
            <xsd:complexType>
              <xsd:complexContent>
                <xsd:extension base="ber:AbstractAssocType">
                  <xsd:sequence>
                    <xsd:element name="ftItem"
                                 type="ber:FeatureTypeRefIdType"/>
                  </xsd:sequence>
                </xsd:extension>
              </xsd:complexContent>
            </xsd:complexType>
          </xsd:element>
          <xsd:element ref="ber:lineSymbolizer" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="type"
                       type="ber:LinearTopologyConnectionVariantType"
                       use="required"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <!-- =========================================================================
       Areal Topology Association
  ========================================================================== -->
  <xsd:complexType name="ArealTopologyAssocType">
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractTopologyAssocType">
        <xsd:sequence>
          <xsd:element name="arealLayerArray">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="arealLayer" maxOccurs="unbounded">
                  <xsd:complexType>
                    <xsd:complexContent>
                      <xsd:extension base="ber:MetaItemType">
                        <xsd:sequence>
                          <xsd:element name="boundaryRole">
                            <xsd:complexType>
                              <xsd:sequence>
                                <xsd:element name="featureType"
                                             type="ber:FeatureTypeRefIdType"
                                             maxOccurs="unbounded"/>
                                <xsd:element name="roleFilter" minOccurs="0" maxOccurs="unbounded">
                                  <xsd:complexType>
                                    <xsd:sequence>
                                      <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                                                   maxOccurs="unbounded" minOccurs="0"/>
                                      <xsd:element ref="ogc:Filter"/>
                                    </xsd:sequence>
                                  </xsd:complexType>
                                </xsd:element>
                              </xsd:sequence>
                            </xsd:complexType>
                          </xsd:element>
                          <xsd:sequence minOccurs="0">
                            <xsd:element name="centroidRole">
                              <xsd:complexType>
                                <xsd:sequence>
                                  <xsd:element name="centroidItem"
                                               maxOccurs="unbounded">
                                    <xsd:complexType>
                                      <xsd:sequence>
                                        <xsd:element name="featureType"
                                                     type="ber:FeatureTypeRefIdType"/>
                                        <xsd:element name="faceFeatureType"
                                                     type="ber:FeatureTypeRefIdType"
                                                     minOccurs="0"/>
                                      </xsd:sequence>
                                    </xsd:complexType>
                                  </xsd:element>
                                  <xsd:element name="roleFilter" minOccurs="0" maxOccurs="unbounded">
                                    <xsd:complexType>
                                      <xsd:sequence>
                                        <xsd:element name="featureType" type="ber:FeatureTypeRefIdType"
                                                     maxOccurs="unbounded" minOccurs="0"/>
                                        <xsd:element ref="ogc:Filter"/>
                                      </xsd:sequence>
                                    </xsd:complexType>
                                  </xsd:element>
                                </xsd:sequence>
                              </xsd:complexType>
                            </xsd:element>
                            <xsd:sequence minOccurs="0">
                              <xsd:element name="faceRole">
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element name="featureType"
                                                 type="ber:FeatureTypeRefIdType"
                                                 maxOccurs="unbounded"/>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:sequence>
                        </xsd:sequence>
                      </xsd:extension>
                    </xsd:complexContent>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

