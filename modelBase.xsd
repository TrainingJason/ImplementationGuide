  <xsd:complexType name="CodeListDataType">
    <xsd:annotation>
      <xsd:documentation>
        CodeList datatype - Refers to codeList Id and display column, it can
        contain reference to sort column
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ber:AbstractDataType">
        <xsd:attribute name="refId" type="xsd:IDREF" use="required">
          <xsd:annotation>
            <xsd:documentation>Refers to codeList Id.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="displayColumn" type="xsd:IDREF" use="required">
          <xsd:annotation>
            <xsd:documentation>
              Refers to codeList's display column.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="codeListRef" type="ber:CodeListDataType"
               substitutionGroup="ber:_dataType">
    <xsd:annotation>
      <xsd:documentation>
        The 'codeListRef' element specifies codelist data type. It has to define
        reference to 'codeList' meta item and its displayable column.
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>

  <xsd:element name="featureRef">
    <xsd:annotation>
      <xsd:documentation>
        The 'featureRef' element is special data type out of general data type
        hierarchy. It represents feature reference data type (formerly known as
        feature code list - like reference). It defines references to a feature
        attribute of master feature type and a relation association type.
        Note: this data type doesn't substitute the abstract element _dataType.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="ber:AbstractDataType">
          <xsd:sequence>
            <xsd:element name="attribute" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>
                  The 'attribute' element specifies a reference to the master
                  feature attribute of related feature. It mustn't be virtual
                  nor code list attribute.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:complexContent>
                  <xsd:extension base="ber:FeatureAttributeRefIdType">
                    <xsd:attribute name="default" type="xsd:boolean">
                      <xsd:annotation>
                        <xsd:documentation>If the featureRef attribute specifies multiple feature attributes, then the optional attribute 'default' identifies which one will be displayed by default.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:complexContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="featureRefRelationAssoc" type="ber:AssocTypeRefIdType">
              <xsd:annotation>
                <xsd:documentation>
                  The 'featureRefAssoc' element specifies a reference to the
                  featureRef association type.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:extension>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

