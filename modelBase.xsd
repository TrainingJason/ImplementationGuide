          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="relationAssoc" type="ber:AssocTypeRefIdType">
        <xsd:annotation>
          <xsd:documentation>
            The 'relationAssoc' element refers to the relation association.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="outputRole">
        <xsd:annotation>
          <xsd:documentation>
            The 'outputRole' element refers to the output role and can contain a
            where clause.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="ber:RoleTypeRefIdType">
              <xsd:sequence>
                <xsd:element name="where" minOccurs="0">
                  <xsd:annotation>
                    <xsd:documentation>
                      The 'where' element defines the query condition for
                      features in the output role.
                    </xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:simpleContent>
                      <xsd:extension base="xsd:string">
                        <xsd:attribute name="alias" type="xsd:NCName">
                          <xsd:annotation>
                            <xsd:documentation>
                              The optional 'alias' attribute specifies string
                              which is used for referring feature attribute
                              (e.g. vedenivn/at_123). Default alias is 'this'.
                            </xsd:documentation>
                          </xsd:annotation>
                        </xsd:attribute>
                      </xsd:extension>
                    </xsd:simpleContent>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType-->

