  <xsd:simpleType name="OutputFormatType">
    <xsd:annotation>
    <xsd:documentation>
      This type defines a number format string. It contains the
      colon with format specifier and precision (:f2 or :F0). Only :F/:f, :N/:n or :D/:d specifiers are supported.
    </xsd:documentation>
  </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value=":[fFnNdD]\d*"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="SystemFeatureAttributeIdType">
    <xsd:annotation>
      <xsd:documentation>
        This type defines the enumeration of system attribute identifiers.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:ID">
      <xsd:enumeration value="id">
        <xsd:annotation>
          <xsd:documentation>
            The feature identifier.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="sid">
        <xsd:annotation>
          <xsd:documentation>
            The semantic identifier.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ftid">
        <xsd:annotation>
          <xsd:documentation>
            The feature type identifier.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="createdBy">
        <xsd:annotation>
          <xsd:documentation>
            Identifies user who this feature created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="updatedBy">
        <xsd:annotation>
          <xsd:documentation>
            Identifies user who this feature modified for the last time.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="createDate">
        <xsd:annotation>
          <xsd:documentation>
            Specifies date when this feature was created.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="updateDate">
        <xsd:annotation>
          <xsd:documentation>
            Specifies date when this feature was modified for the last time.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="longTransactionId">
        <xsd:annotation>
          <xsd:documentation>
            Specifies the identifier of a long transaction.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="symbologyTokens">
        <xsd:annotation>
          <xsd:documentation>
            Specifies the system attribute for dynamic symbology facility.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="validFrom">
        <xsd:annotation>
          <xsd:documentation>Identifies the beginning of the record's validity.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="validTo">
        <xsd:annotation>
          <xsd:documentation>Identifies the record's expiration date.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="featureInfo">
        <xsd:annotation>
          <xsd:documentation>Identifies user readable id.</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
